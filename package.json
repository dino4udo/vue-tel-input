{
  "name": "vue-tel-input",
  "version": "4.4.0",
  "description": "International Telephone Input with Vue",
  "author": "Steven Dao <iamstevendao@gmail.com>",
  "license": "MIT",
  "homepage": "https://educationlink.github.io/vue-tel-input/",
  "repository": {
    "type": "git",
    "url": "https://github.com/EducationLink/vue-tel-input.git"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "demo": "vue-cli-service build",
    "build": "npm run build:browser && npm run build:es && npm run build:umd && npm run build:scss",
    "build:browser": "cross-env NODE_ENV=production rollup --config build/rollup.config.browser.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.es.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.umd.js",
    "build:scss": "node-sass --output-style expanded src/scss/vue-tel-input.scss > dist/vue-tel-input.css ",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "files": [
    "dist/",
    "src/scss/"
  ],
  "main": "dist/vue-tel-input.umd.js",
  "module": "dist/vue-tel-input.esm.js",
  "unpkg": "dist/vue-tel-input.min.js",
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "awesome-phonenumber": "^2.32.0",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "~4.2.2",
    "@vue/cli-plugin-eslint": "~4.2.2",
    "@vue/cli-plugin-unit-jest": "~4.2.2",
    "@vue/cli-service": "~4.2.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^2.10.2",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.7",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "telephone",
    "phone",
    "vue-tel-input",
    "phone input",
    "telephone input",
    "input",
    "international phone"
  ]
}
